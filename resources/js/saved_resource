return isEventSupported()?'wheel':'mousewheel';},normalize:function normalize(e){var PIXEL_STEP=10;var LINE_HEIGHT=40;var PAGE_HEIGHT=800;var sX=0;var sY=0;var pX=0;var pY=0;if('detail'in e){sY=e.detail;}
